{
  "manifest_version": 3,
  "name": "Copy Title or Url",
  "version": "1.6",
  "description": "For copying title or url.",
  "icons": {
    "64": "icon.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "clipboardWrite",
    "tabs"
  ],
  "action": {
    "default_title": "Copy Title or Url",
    "default_popup": "index.html"
  }
}
